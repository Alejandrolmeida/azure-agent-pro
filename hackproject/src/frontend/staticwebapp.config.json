{
  "routes": [
    {
      "route": "/api/*",
      "allowedRoles": ["anonymous"]
    }
  ],
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/images/*.{png,jpg,gif}", "/css/*", "/js/*"]
  },
  "responseOverrides": {
    "404": {
      "rewrite": "/index.html",
      "statusCode": 200
    }
  },
  "globalHeaders": {
    "content-security-policy": "default-src 'self' https: 'unsafe-eval' 'unsafe-inline'; img-src 'self' data: https:; font-src 'self' data: https:;"
  },
  "mimeTypes": {
    ".json": "application/json",
    ".geojson": "application/json"
  }
}
